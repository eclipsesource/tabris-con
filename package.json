{
  "title": "Tabris Con",
  "description": "A template for conference apps built with Tabris.js",
  "version": "2.0.0",
  "main": "dist",
  "scripts": {
    "test": "ts-mocha -p ./test/tsconfig.json ./test/**/*.spec.js",
    "test:file": "ts-mocha -p ./test/tsconfig.json",
    "posttest": "npm run lint",
    "lint": "tslint --project . 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:file": "tslint --project .",
    "pre-build": "npm run clean",
    "build": "npm run pre-build && webpack",
    "clean": "rm -rf dist/",
    "watch": "webpack -w",
    "start": "tabris serve"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment-timezone": "^0.5.14",
    "promise.prototype.finally": "^1.0.1",
    "sanitize-html": "^1.11.3",
    "tabris": "^2.4.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.92",
    "@types/moment-timezone": "^0.5.3",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "chai-things": "^0.2.0",
    "fetch-mock": "^4.1.0",
    "mocha": "^2.3.4",
    "mock-fs": "^4.4.1",
    "node-fetch": "^1.4.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "tabris-decorators": "^1.0.0-test2",
    "ts-loader": "^3.2.0",
    "ts-mocha": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "webpack": "^3.10.0"
  }
}
